journal = {Journal of Fluids and Structures},
mendeley-groups = {PhD/Hydraulic actuation/Fluid flow through collapsed tubes},
number = {6},
pages = {565--599},
title = {{Large post-buckling deformations of cylindrical shells conveying viscous flow}},
volume = {10},
year = {1996}
}
