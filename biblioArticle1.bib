issn = {0950-1207},
journal = {Proceedings of the Royal Society of London. Series A, Containing Papers of a Mathematical and Physical Character},
number = {563},
pages = {366--378},
title = {{On the flow of water through pipes and passages having converging or diverging boundaries}},
volume = {83},
year = {1910}
}

