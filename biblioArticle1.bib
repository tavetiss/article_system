@article{Azimi2021,
abstract = {Self-powered biomedical implants improve the life of patients and lower the risks associated with battery replacement. Piezoelectric energy harvesters that generate electricity from the cardiac motions are among the potential candidates to be used in self-powered implants, such as cardiac pacemakers. In this context, lead-based ceramic piezoelectric nanogenerators (PNGs) were emerged, which are toxic and susceptible to fatigue crack, causing harm to the patients. Polyvinylidene fluoride-trifluoroethylene (PVDF-TrFE)-based films were also developed as cardiac energy harvesters. Here, we show a battery-free heart pacemaker that is powered by the generated electricity of a biocompatible and flexible piezoelectric polymer-based nanogenerator (PNG) from the cardiac motions of the left ventricle. The PNG is comprised of composite nanofibers of poly(vinylidene fluoride) (PVDF) and a hybrid nanofiller made of zinc oxide (ZnO) and reduced graphene oxide (rGO). The composite nanofiber is optimized towards achieving a large power output. In vivo implanted optimized PNG can successfully harvest 0.487 $\mu$J from every heartbeat, which is conveniently larger than the pacing threshold energy for the human heart. The successful demonstration of a self-powered pacemaker places the polymer-based PNGs among the viable candidates for self-powered biomedical implants.},
author = {Azimi, Sara and Golabchi, Allahyar and Nekookar, Abdolhossein and Rabbani, Shahram and Amiri, Morteza Hassanpour and Asadi, Kamal and Abolhasani, Mohammad Mahdi},
doi = {10.1016/j.nanoen.2021.105781},
file = {:D\:/PHD/Biblio PhD/Energy-harvesting/Human body/azimi2021.pdf:pdf},
issn = {22112855},
journal = {Nano Energy},
keywords = {Biomechanical energy harvesting,PVDF composite fibers,Piezoelectric nanogenerators,Self-powered implantable medical electronics},
mendeley-groups = {PhD/Energy harvesting/Human body},
number = {November 2020},
pages = {105781},
publisher = {Elsevier Ltd},
title = {{Self-powered cardiac pacemaker by piezoelectric polymer nanogenerator implant}},
url = {https://doi.org/10.1016/j.nanoen.2021.105781},
volume = {83},
year = {2021}
}
@article{Almouahed2016,
abstract = {A self-powered device that can be embedded within the knee implant to measure tibiofemoral forces is presented. This device can be used intraoperatively during the knee surgery or postoperatively through the implant lifetime. The twofold use of piezoceramic elements simultaneously as force sensors and power harvesters makes the device small enough to fit into the tibial component without increasing its thickness excessively. In this paper, we present the custom-designed tibial component developed to accommodate the double-function piezoceramics. In addition, we quantify experimentally the mean electric power harvested by these elements during one cycle of a simulated human walking. The power conditioning circuits and radiofrequency link needed for the function of this device is briefly addressed and will be fully discussed in a later paper.},
author = {Almouahed, Shaban and Hamitouche, Chafiaa and Stindel, Eric},
