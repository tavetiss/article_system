number = {4},
pages = {3437--3450},
publisher = {Faculty of Engineering, Alexandria University},
title = {{Analytical and numerical investigations of the collapse of blood vessels with nonlinear wall material embedded in nonlinear soft tissues}},
url = {https://doi.org/10.1016/j.aej.2018.03.002},
volume = {57},
year = {2018}
}
