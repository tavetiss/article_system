doi = {10.1109/JMEMS.2010.2093562},
file = {:C\:/Users/avetisst/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee, Liamini, Frechette - 2011 - A Silicon Microturbopump for a Rankine-Cycle Power-Generation Microsystem—Part II Fabrication and Chara.pdf:pdf},
issn = {1057-7157},
journal = {Journal of Microelectromechanical Systems},
keywords = {Blades,Etching,Fabrication,Heat engines,Pyrex glass wafer,Rankine cycle,Rotors,SOI wafers,Silicon,Turbines,anodic bonding,bearings,fusion bonding,ion etching,machine bearings,manual assembly,microfabrication,microfluidics,micromachine,micromachining,micropumps,power 0.073 W,power 0.38 W,power generation,power generation microsystem,pressure 41 kPa,pressure 88 kPa,rotor,rotors,silicon microturbopump,size 4 mm,small electric machines,sputter etching,turbines,turbogenerators},
