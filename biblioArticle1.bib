@article{Cadei2014,
abstract = {Implantable medical devices usually require a battery to operate and this can represent a severe restriction. In most cases, the implantable medical devices must be surgically replaced because of the dead batteries; therefore, the longevity of the whole implantable medical device is determined by the battery lifespan. For this reason, researchers have been studying energy harvesting techniques from the human body in order to obtain batteryless implantable medical devices. The human body is a rich source of energy and this energy can be harvested from body heat, breathing, arm motion, leg motion or the motion of other body parts produced during walking or any other activity. In particular, the main human-body energy sources are kinetic energy and thermal energy. This paper reviews the state-of-art in kinetic and thermoelectric energy harvesters for powering implantable medical devices. Kinetic energy harvesters are based on electromagnetic, electrostatic and piezoelectric conversion. The different energy harvesters are analyzed highlighting their sizes, energy or power they produce and their relative applications. As they must be implanted, energy harvesting devices must be limited in size, typically about 1 cm 3. The available energy depends on human-body positions; therefore, some positions are more advantageous than others. For example, favorable positions for piezoelectric harvesters are hip, knee and ankle where forces are significant. The energy harvesters here reported produce a power between 6 nW and 7.2 mW; these values are comparable with the supply requirements of the most common implantable medical devices; this demonstrates that energy harvesting techniques is a valid solution to design batteryless implantable medical devices. {\textcopyright} 2014 IOP Publishing Ltd.},
author = {Cadei, Andrea and Dionisi, Alessandro and Sardini, Emilio and Serpelloni, Mauro},
doi = {10.1088/0957-0233/25/1/012003},
file = {:D\:/PHD/Biblio PhD/Energy-harvesting/EAR - HEAD/Kinetic/cadei2013.pdf:pdf},
issn = {13616501},
journal = {Measurement Science and Technology},
keywords = {batteryless devices,electromagnetic generators,electrostatic generators,energy harvesting,implantable medical devices,micropower generator,piezoelectric generators,self-powered systems,thermoelectric generator},
number = {1},
title = {{Kinetic and thermal energy harvesters for implantable medical devices and biomedical autonomous sensors}},
volume = {25},
year = {2014}
}
@inproceedings{Chaudhuri2017,
abstract = {Efficient and uninterrupted operation of the implanted biomedical devices such as Cochlear implants demand integrated energy harvester. MEMS based piezoelectric energy harvesters are a potential solution. In a Cochlear implant the harvester has to be placed on the eardrum which vibrates as the sound waves enter the ear. If the frequency of the vibration of the eardrum matches with the resonant frequency of the energy harvester, it generates maximum voltage output which can be used to power the battery of the Cochlear implant. In this paper a novel energy harvester is designed with bio-compatible Zinc Oxide thin film (thickness 2 $\mu$m) as the piezoelectric material. The harvester is in the shape of a tapered cantilever beam with a proof mass to reduce the resonant frequency. Moreover, a perforation has been implemented on the cantilever beam in a suitable position to further reduce the resonant frequency and to increase the voltage output without increasing the length of the cantilever. The achieved resonant frequency of the final proposed structure is 126 Hz. The voltage obtained is 0.75 V at an acceleration of 0.1g at the resonant frequency. The proposed structure can be fitted easily on the eardrum and used to generate voltage from the vibration of the eardrum caused by incoming sound waves and can replace the need of recharging the battery in the Cochlear implants.},
author = {Chaudhuri, Dipta and Kundu, Sudip and Chattoraj, Neela},
booktitle = {2017 Innovations in Power and Advanced Computing Technologies, i-PACT 2017},
doi = {10.1109/IPACT.2017.8245043},
file = {:D\:/PHD/Biblio PhD/Energy-harvesting/EAR - HEAD/Kinetic/chaudhuri2017.pdf:pdf},
isbn = {9781509056828},
keywords = {Cochlear,Eardrum,Energy Harvesting,Perforated},
month = {apr},
pages = {1--5},
publisher = {Institute of Electrical and Electronics Engineers Inc.},
title = {{Harvesting energy with zinc oxide bio-compatible piezoelectric material for powering cochlear implants}},
url = {http://ieeexplore.ieee.org/document/8245043/},
volume = {2017-Janua},
year = {2017}
}
@inproceedings{Pellegrini2013,
abstract = {Powering electronics without depending on batteries is an open research field. Mechanical vibrations prove to be a reliable energy source, but low-frequency broadband vibrations cannot be harvested effectively using linear oscillators. This article discusses an alternative for harvesting such vibrations, with energy harvesters with two stable configurations. The challenges related to nonlinear dynamics are briefly discussed. Different existing designs of bistable energy harvesters are presented and classified, according to their feasibility for miniaturization. A general dynamic model for those designs is described. Finally, an extensive discussion on quantitative measures of evaluating the effectiveness of energy harvesters is accomplished, resulting in the proposition of a new dimensionless metric suited for a broadband analysis. {\textcopyright} The Author(s) 2012.},
