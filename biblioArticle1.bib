@article{Huguet2018,
abstract = {In the aim of giving an alternative to chemical batteries for energy supply of wireless autonomous devices, the present work focuses on ambient vibration energy harvesting using harvesters taking benefit of energy stored in an oscillating mass subjected to acceleration. More particularly, the attention is put on nonlinear bistable harvesters which offer a wider bandwidth than linear ones. However, due to their nonlinearities, these bistable harvesters exhibit different coexisting behaviors (orbits) on their operating frequency range. Only some of them are interesting for energy harvesting because of their high amplitude of oscillations inducing high energy levels (high orbits). Nevertheless, those high orbits are coexisting with low orbits (i.e., low harvestable energy) on a major portion of their frequency range and thus are not automatically reached. This work hence introduce new strategies to experience orbit jumps from low to high orbits playing with different parameters of the bistable harvester. Preliminary analyses based on energy considerations demonstrates that the most technically achievable strategy, adopted for the experimental analysis, is the orbit jump with fast modifications of its buckling level. More particularly, the bistable harvester is first quickly over-buckled at a particular instant and then quickly released to its initial buckling level when the mass reaches a maximum of displacement (in order to maximize the potential energy brought to the mass). Two elements of this strategy were adjustable: the amplitude of the buckling level variation and the instant at which this change starts. Experimental results show that choosing a good combination of those two elements leads to a high probability to jump from low to high orbits on the whole frequency range concerned by high orbits (from 70% chance to 100% chance to jump). Thanks to this orbit jump technique, the high orbits can be ensured for the considered bistable harvester on a continuous wide frequency range of 50 Hz (from 20 Hz to 70 Hz) on which the mean harvested power varies from 20 $\mu$W to 500 $\mu$W. Finally, it is shown that the energy consumed to ensure the orbit jump can be recovered within 2 s.},
author = {Huguet, Thomas and Lallart, Micka{\"{e}}l and Badel, Adrien},
doi = {10.1016/j.ymssp.2019.03.051},
file = {:D\:/PHD/Biblio PhD/Energy-harvesting/Piezo/huguet2018.pdf:pdf},
issn = {10961216},
journal = {Mechanical Systems and Signal Processing},
keywords = {Bistable,Energy harvesting,High,Jump,Nonlinear,Orbit,Vibration},
mendeley-groups = {PhD/Energy harvesting/Piezoelectric},
pages = {202--215},
publisher = {Elsevier Ltd},
title = {{Orbit jump in bistable energy harvesters through buckling level modification}},
url = {https://doi.org/10.1016/j.ymssp.2019.03.051},
volume = {128},
year = {2019}
}
@article{Wang2020,
abstract = {Micro check valves play a pivotal role in many microfluidic systems. However, there is still an unmet need for low-cost check valves with good performance and easy integration. In this work, we present a planar micro check valve using a hydrogel valve core fabricated in situ in a microchannel. This valve is characterized by a simple fabrication process and easy integration with other microfluidic devices. Experimental results proved that the check valve performs better than most existing in-plane micro check valves, especially in low-pressure operations, with nearly zero forward cracking pressure and reverse leakage, adjustable forward fluidic resistance, and good repeatability. We further proposed a normally-closed check valve with controllable cracking pressure to meet different requirements of microfluidic devices. Precise cracking pressures could be achieved by regulating the valve spring width. To demonstrate the valve usefulness, we employed these valves to make a finger-actuated micro-mixer and a bio-actuated micro-pump. This study makes the flow control more simplified in integrated microfluidic systems.},
author = {Wang, Yingzhe and Toyoda, Kazuma and Uesugi, Kaoru and Morishima, Keisuke},
doi = {10.1016/j.sna.2020.111878},
file = {:D\:/PHD/Biblio PhD/Actionnement hydraulique/wang2020.pdf:pdf},
issn = {09244247},
journal = {Sensors and Actuators, A: Physical},
keywords = {Check valve,Earthworm,Hydrogel,Microfluidics,Optofluidic lithography},
mendeley-groups = {PhD/Hydraulic actuation},
pages = {111878},
publisher = {Elsevier B.V.},
title = {{A simple micro check valve using a photo-patterned hydrogel valve core}},
url = {https://doi.org/10.1016/j.sna.2020.111878},
volume = {304},
year = {2020}
}
@article{Avetissian2021,
abstract = {An energy harvester device associated to an in-ear device and harvesting energy from dynamic motion of the wall of the ear canal receiving a dynamic motion to hydraulic pressure converter therein, with an external sheath of the converter generally assuming the5 contour of the ear canal wall. The converter has a hydraulic pump generating hydraulic flow rate within a hydraulic circuit configured to actuate a bistable oscillator. The energy harvester device generates an electric voltage as the bistable oscillator is translated between its stable states and oscillates around each of them thereby generating a variable mechanical stress in a piezoelectric device. The electric charge may thereafter be stored in an energy10 storage module of the in-ear device.},
